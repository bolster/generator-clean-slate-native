{
  "name": "<%= slugname %>",
  "version": "0.0.0",
  "description": "<%= props.description %>",
  <% if (props.homepage) { %>
  "homepage": "<%= props.homepage %>",<% } %>
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/react-native-webpack-server start",
    "hot": "HOT=1 ./node_modules/.bin/react-native-webpack-server start --hot"
  },
  "repository": "<%= repoUrl %>",
  "author": {
    "name": "<%= props.authorName %>",
    "email": "<%= props.authorEmail %>"<% if (props.authorUrl) { %>,
    "url": "<%= props.authorUrl %>"<% } %>
  },
  "dependencies": {
    "react-native": "^0.6.0",
    "react": "^0.13.3",
    "lodash": "^3.8.0",
    "moment": "^2.10.3",
    "immutable": "^3.7.4",
    "validator": "^3.40.1",
    "bluebird": "^2.9.30",
    "react-native-device": "^0.2.0",
    "react-native-keyboardevents": "^0.4.2"
  },
  "devDependencies": {
    "babel": "^5.6.7",
    "babel-core": "^5.6.7",
    "babel-eslint": "^3.1.18",
    "babel-loader": "^5.2.0",
    "babel-runtime": "^5.6.7",
    "eslint": "^0.23.0",
    "eslint-plugin-react": "^2.5.2",
    "eventemitter3": "^1.1.1",
    "react-native-browser-polyfill": "^0.1.1",
    "react-native-webpack-server": "0.2.2",
    "react-pure-render": "^1.0.2",
    "whatwg-fetch": "^0.9.0"
  }
}
